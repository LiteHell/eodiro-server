{
  "name": "eodiro-api2",
  "version": "1.0.0-alpha.1",
  "main": "src/index.ts",
  "repository": "https://github.com/paywteam/eodiro-api2.git",
  "author": "PAYW",
  "license": "MIT",
  "scripts": {
    "build": "npm run clear && tsc",
    "dev": "npm run clear && npm run set-pkg-dev && nodemon --exec cross-env NODE_ENV=development ts-node -- src/index.ts",
    "start": "npm run set-pkg-start && node build/src/index.js",
    "start-dev": "npm run set-pkg-start && NODE_ENV=development node build/src/index.js",
    "pm2-start": "npm run set-pkg-start && pm2 start build/src/index.js --name \"eodiro-api2\"",
    "pm2-restart": "pm2 restart eodiro-api2",
    "pm2-stop": "pm2 stop eodiro-api2",
    "pm2-delete": "pm2 delete eodiro-api2",
    "set-pkg-start": "ts-node src/dev/modify-package.ts",
    "set-pkg-dev": "cross-env NODE_ENV=development ts-node src/dev/modify-package.ts",
    "clear": "rimraf build/*",
    "test": "mocha -r ts-node/register -r module-alias/register test/**/*.spec.ts",
    "dev-legacy": "npm run clear && nodemon -e ts --exec \"npm run compile\""
  },
  "_moduleAliases": {
    "@": "src",
    "@@": "."
  },
  "dependencies": {
    "@payw/cau-cafeteria-menus-scraper": "^2.1.1",
    "@payw/cau-timetable-scraper": "^2.0.0-beta.2",
    "@types/multer": "^1.4.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dayjs": "^1.8.19",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "niceware": "^2.0.0",
    "nodemailer": "^6.4.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.8",
    "@types/cron": "^1.7.1",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mocha": "^7.0.1",
    "@types/mysql": "^2.15.8",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
