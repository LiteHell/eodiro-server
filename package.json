{
  "name": "eodiro-api-mysql",
  "version": "1.0.0-alpha.1",
  "main": "src/index.ts",
  "repository": "https://github.com/paywteam/eodiro-api-mysql.git",
  "author": "PAYW",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node build/src/index.js",
    "compile": "tsc --incremental --sourceMap && node build/src/index.js",
    "dev": "yarn start && tsc -w --incremental",
    "dev:ts": "yarn clear && nodemon --exec ts-node -- src/index.ts",
    "dev:legacy": "yarn clear && nodemon -e ts --exec \"yarn run compile\"",
    "clear": "rimraf build/*"
  },
  "_moduleAliases": {
    "@": "build/src",
    "@@": "build"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dayjs": "^1.8.16",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "module-alias": "^2.2.2",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.15",
    "@types/mysql": "^2.15.7",
    "@types/nodemailer": "^6.2.2",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.5.1",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
}
