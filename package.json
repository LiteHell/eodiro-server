{
  "name": "eodiro-api-mysql",
  "version": "1.0.0-alpha.1",
  "main": "src/index.ts",
  "repository": "https://github.com/paywteam/eodiro-api-mysql.git",
  "author": "PAYW",
  "license": "MIT",
  "scripts": {
    "build": "yarn package:start && tsc",
    "start": "yarn package:start && node build/src/index.js",
    "start:pm2": "yarn package:start && pm2 start build/src/index.js --name \"eodiro-api2\"",
    "restart": "pm2 restart eodiro-api2",
    "stop": "pm2 stop eodiro-api2",
    "delete": "pm2 delete eodiro-api2",
    "dev": "yarn clear && yarn package:dev && nodemon --exec cross-env NODE_ENV=development ts-node -- src/index.ts",
    "package:start": "ts-node src/dev/modify-package.ts",
    "package:dev": "cross-env NODE_ENV=development ts-node src/dev/modify-package.ts",
    "compile": "tsc --incremental --sourceMap && cross-env NODE_ENV=development node build/src/index.js",
    "dev:legacy": "yarn clear && nodemon -e ts --exec \"yarn run compile\"",
    "clear": "rimraf build/*",
    "test": "mocha -r ts-node"
  },
  "_moduleAliases": {
    "@": "src",
    "@@": "."
  },
  "dependencies": {
    "@payw/cau-cafeteria-menus-scraper": "^2.1.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "dayjs": "^1.8.16",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/cron": "^1.7.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.15",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mocha": "^5.2.7",
    "@types/mysql": "^2.15.7",
    "@types/nodemailer": "^6.2.2",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.5.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
}
